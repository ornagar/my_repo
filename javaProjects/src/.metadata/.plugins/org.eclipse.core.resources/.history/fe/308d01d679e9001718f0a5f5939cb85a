package il.co.ilrd;

public class Singleton1 {
	private static Singleton1 instance = new Singleton1();
	
	public static Singleton1 getInstance() {
		return instance;
	}
	
	public void foo1() { 
		System.out.println("inside foo1()"); 
	}
	
	public static void foo14() {
		System.out.println("hi");
	}
	
	private Singleton1() {}
}
