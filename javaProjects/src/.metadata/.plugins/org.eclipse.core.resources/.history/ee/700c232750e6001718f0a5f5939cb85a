package il.co.ilrd;

public class Rectangle extends Shape {
	private double width;
	private double length;
	
	public Rectangle() {
		super();
		width = length = 1.0;
	}
	
	public Rectangle(double width, double length) {
		super();
		this.width = width;
		this.length = length;
	}
	
	public Rectangle(double width, double length, String color, boolean filled) {
		super(color, filled);
		this.width = width;
		this.length = length;
	}
}
