package il.co.ilrd;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class TestLinkedList {

	@Test
	void testList1() {
		LinkedList<Integer> list1 = new LinkedList<>();
		
		for (int i = 0; i < 50; ++i) {
			list1.pushFront(i);
		}
		
		
		
		LinkedList<String> list2 = new LinkedList<>();
		
		for (Byte i = 0; i < 50; ++i) {
			list2.pushFront(Integer.toString(i));
		}
		LinkedList<Integer>.ListIter iter1 = list1.find(12);
		LinkedList<String>.ListIter iter2 = list2.find("12");
		
		System.out.println(iter2.getData());
		assertTrue(iter2.getData().equals("12"));
		assertTrue(iter1.getData().equals(12));
		
		while (!list1.isEmpty()) {
			System.out.println(list1.popFront());
			System.out.println(list2.popFront());
		}
	}

}
