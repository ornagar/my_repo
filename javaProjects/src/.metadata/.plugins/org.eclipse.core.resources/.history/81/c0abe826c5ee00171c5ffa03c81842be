package il.co.ilrd;

import java.util.*;


//import java.io.File;

public class TestFS {
	public static void main(String[] args) {
		Entry root = Entry.buildTree(args[0]);
		root.printTree();
		
		String[] strings = {"hello", "how", "are", "you"};
		Set<String> set = new LinkedHashSet<String>();
		
		for (String i : strings) {
			set.add(i);
		}
		
		System.out.println(set);
	}
}
